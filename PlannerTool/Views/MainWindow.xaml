<Window x:Class="PlannerTool.Views.MainWindow"
        x:Name="MainWindowUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PlannerTool.Views"
        xmlns:vm="clr-namespace:PlannerTool.ViewModels"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <vm:MainWindowViewModel/>
    </Window.DataContext>
    <Grid>
        <Button Margin="150" Content="Press" Command="{Binding CommandProp}" CommandParameter="{Binding ElementName=MainWindowUI}"/>

    </Grid>
</Window>
